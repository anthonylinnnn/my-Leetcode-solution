bool threeConsecutiveOdds(int* arr, int arrSize) {
    
    int n = arrSize;
    int temp = 0;
    for(int i = 0; i < n - 2; i++){
        if(arr[i] % 2 != 0 && arr[i + 1] % 2 != 0 && arr[i + 2] % 2 != 0){
            printf("arr[i] = %d, arr[i+1] = %d, arr[i+2] = %d", arr[i], arr[i+1], arr[i+2]);
            temp++;
        }
        else{
            printf("arr[i] = %d, arr[i+1] = %d, arr[i+2] = %d", arr[i], arr[i+1], arr[i+2]);
        }
    }
    if(temp > 0){
        return true;
    }
    else{
        return false;
    }
}
